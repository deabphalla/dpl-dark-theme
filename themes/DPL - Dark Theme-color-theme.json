	"tokenColors": [
		// Python
		{
			"scope": [
				"variable",                     // General variable
				"variable.other.constant",      // Other constant variable
				"source.python",                // The whole source
			],
			"settings": {
				"foreground": "#e0e0e0",
			},
		},
		{
			"scope": [
				"entity.name.function",         // Function
			],
			"settings": {
				"foreground": "#56cdd6",
			},
		},
		{
			"scope": [
				"string.quoted.docstring.multi.python",  // Comment Doctstring
				"comment.line.number-sign.python"        // Comment Single Line
			],
			"settings": {
				"foreground": "#555555",
			},
		},
		{
			"scope": [
				"meta.function-call.generic.python",    // Buildin function call
				"entity.name.type.class",               // Class
				"entity.name.namespace",                // Namespace from import
				"variable.parameter",                   // Parameter in function
			],
			"settings": {
				"foreground": "#569CD6",
			},
		},
		{
			"scope": [
				"keyword.control.import.python",        // Import module
				"meta.attribute.python",                // Object Attribute
				"storage.type.function.python",         // Function lead (def ...)
				"storage.type.class.python",            // Class lead (class ...)
				"keyword.control.flow.python",          // Control flow (if else, for, ...)
			],
			"settings": {
				"foreground": "#C586C0",
			},
		},
		{
			"scope": [
				"string.quoted.single.python"           // String

			],
			"settings": {
				"foreground": "#c770d3",
			},
		},
		// ITALIC
		{
			"scope": [
				"variable.parameter",                       // Parameter
				"string.quoted.docstring.multi.python",     // Comment Doctstring
				"comment.line.number-sign.python",          // Comment Single Line

			],
			"settings": {
				"fontStyle": "italic",
			},
		},// Markdown
		{
			"scope": [
				"heading.1.markdown", // Markdown Header 1
				"heading.2.markdown", // Markdown Header 2
			],
			"settings": {
				"foreground": "#4caf50",
				"fontStyle": "bold",
			},
		},
		{
			"scope": [
				"heading.3.markdown", // Markdown Header 3
			],
			"settings": {
				"foreground": "#2196f3"
			},
		}, {
			"scope": [
				"heading.4.markdown", // Markdown Header 4
				"heading.5.markdown", // Markdown Header 5
			],
			"settings": {
				"foreground": "#ed4b82"
			},
		},
		{
			"scope": [
				"markup.list.unnumbered.markdown", // Markdown List Unnumbered
				"markup.list.numbered.markdown", // Markdown List Numbered
			],
			"settings": {
				"foreground": "#c9c9c9"
			},
		},
		{
			"scope": [
				"fenced_code.block.language.markdown",
				"punctuation.definition.markdown", // Markdown Block Code
			],
			"settings": {
				"foreground": "#f13acd",
				"fontStyle": "italic bold"
			},
		},
		{
			"scope": [
				"markup.fenced_code.block.markdown", // Markdown Block Code Content
			],
			"settings": {
				"foreground": "#c9c9c9",
				"fontStyle": "italic"
			},
		},
		{
			"scope": [
				"markup.bold.markdown",
			],
			"settings": {
				"foreground": "#955cf4",
			},
		},
		{
			"scope": [
				"punctuation.definition.link.title.begin.markdown",
				"punctuation.definition.link.title.end.markdown",
			],
			"settings": {
				"foreground": "#75a0ef",
			},
		},
		{
			"scope": [
				"string.other.link.title.markdown"
			],
			"settings": {
				"foreground": "#955cf4",
				"fontStyle": "bold"
			},
		},
		{
			"scope": [
				"markup.inline.raw.string.markdown"
			],
			"settings": {
				"foreground": "#929396",
			},
		},
		// SQL
		{
			"scope": [
				"support.function.aggregate.sql"
			],
			"settings": {
				"foreground": "#19c9c9", // Markdown Block Code
			},
		},
		{
			"scope": [
				"keyword.other.sql",
				"keyword.other.DML.sql",
				"keyword.plsql.mysql",
				"keyword.other.alias.sql"
			],
			"settings": {
				"foreground": "#c660cd",
			},
		},
		{
			"scope": [
				"string.quoted.double.sql", // String
				"string.quoted.single.sql"
			],
			"settings": {
				"foreground": "#6787c3",
			},
		},
	]
}
