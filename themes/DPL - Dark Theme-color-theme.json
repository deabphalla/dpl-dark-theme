	"tokenColors": [
		// Python
		{
			"scope": [
				"source.python", // The whole source
			],
			"settings": {
				"foreground": "#ededed",
			},
		},
		{
			"scope": [
				"meta.function.python",
				"keyword.operator.assignment.python",
			],
			"settings": {
				"foreground": "#ee6bf8",s
			},
		},
		{
			"scope": [
				"keyword",
				"meta.function-call.generic.python", // function-call.generic
				"entity.name.type.class.python", // Class
				"meta.member.access.python", // Member Access
			],
			"settings": {
				"foreground": "#5ca9dc",
			},
		},
		{
			"scope": [
				"variable.parameter.function.language.special.self.python", // self.
			],
			"settings": {
				"foreground": "#19c9c9",
				"fontStyle": "italic"
			},
		},
		{
			"scope": [
				"meta.function.decorator.python", // Decorator
			],
			"settings": {
				"foreground": "#54b858",
				"fontStyle": "italic"
			},
		},
		{
			"scope": [
				"variable.parameter.function-call.python",
			],
			"settings": {
				"fontStyle": "italic"
			},
		},
		{
			"scope": [
				"comment.line.number-sign.python", // Comment Line
				"string.quoted.docstring.multi.python" // Docstring
			],
			"settings": {
				"foreground": "#898989",
				"fontStyle": "italic"
			},
		},
		{
			"scope": [
				"meta.attribute.python", // Attribute
			],
			"settings": {
				"foreground": "#ead955",
			},
		},
		{
			"scope": [
				"keyword.control.import.python",
			],
			"settings": {
				"foreground": "#ee6bf8",
			},
		},
		// Markdown
		{
			"scope": [
				"heading.1.markdown", // Markdown Header 1 & 2
				"heading.2.markdown",
			],
			"settings": {
				"foreground": "#4caf50",
				"fontStyle": "bold",
			},
		},
		{
			"scope": [
				"heading.3.markdown", // Markdown Header 3 & 4
				"heading.4.markdown",
				"heading.5.markdown",
			],
			"settings": {
				"foreground": "#2196f3"
			},
		},
		{
			"scope": [
				"markup.list.unnumbered.markdown", // Markdown List Unnumbered
				"markup.list.numbered.markdown", // Markdown List Numbered
			],
			"settings": {
				"foreground": "#c9c9c9"
			},
		},
		{
			"scope": [
				"fenced_code.block.language.markdown",
				"punctuation.definition.markdown", // Markdown Block Code
			],
			"settings": {
				"foreground": "#f13acd",
				"fontStyle": "italic bold"
			},
		},
		{
			"scope": [
				"markup.fenced_code.block.markdown", // Markdown Block Code Content
			],
			"settings": {
				"foreground": "#c9c9c9",
				"fontStyle": "italic"
			},
		},
		{
			"scope": [
				"markup.bold.markdown",
			],
			"settings": {
				"foreground": "#955cf4",
			},
		},
		{
			"scope": [
				"punctuation.definition.link.title.begin.markdown",
				"punctuation.definition.link.title.end.markdown",
			],
			"settings": {
				"foreground": "#75a0ef",
			},
		},
		{
			"scope": [
				"string.other.link.title.markdown"
			],
			"settings": {
				"foreground": "#955cf4",
				"fontStyle": "bold"
			},
		},
		{
			"scope": [
				"markup.inline.raw.string.markdown"
			],
			"settings": {
				"foreground": "#929396",
			},
		},
		// SQL
		{
			"scope": [
				"support.function.aggregate.sql"
			],
			"settings": {
				"foreground": "#19c9c9", // Markdown Block Code
			},
		},
		{
			"scope": [
				"keyword.other.sql",
				"keyword.other.DML.sql",
				"keyword.plsql.mysql",
				"keyword.other.alias.sql"
			],
			"settings": {
				"foreground": "#ee6bf8",
			},
		},
		{
			"scope": [
				"string.quoted.double.sql", // String
				"string.quoted.single.sql"
			],
			"settings": {
				"foreground": "#75a0ef",
			},
		},
	]
}
